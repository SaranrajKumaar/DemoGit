package Interview;

import java.util.Scanner;

public class Triangle {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int count  = 5;
		
		
		int i,j;
		
		for  (i=0;i<count;i++) {
			for(j=0; j<=count;j++) {
				System.out.print("* ");
			}
			System.out.println();
			
			
		}
		
		
		
		
		
		//*******
		


	}
	

}
